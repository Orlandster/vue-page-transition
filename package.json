    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "core-js": "^3.3.2",
    "np": "^7.0.0",
